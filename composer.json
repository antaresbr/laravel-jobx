{
    "name": "antaresbr/laravel-jobx",
    "description": "A custom job package based on Laravel Framework",
    "homepage": "https://github.com/antaresbr/laravel-jobx",
    "keywords": [
        "php",
        "laravel",
        "job"
    ],
    "authors": [
        {
            "name": "Eduardo Guerra",
            "email": "eduardo.antares@gmail.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8",
        "antaresbr/foundation": "*",
        "antaresbr/laravel-http": "^9",
        "antaresbr/laravel-multienv": "^9",
        "antaresbr/laravel-socket": "^9",
        "laravel/framework": "^9",
        "nesbot/carbon": "*",
        "symfony/process": "*"
    },
    "autoload": {
        "files": [
            "support/helpers.php"
        ],
        "psr-4": {
            "Antares\\Jobx\\": "src/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^7"
    },
    "autoload-dev": {
        "psr-4": {
            "Antares\\Jobx\\Database\\Factories\\": "database/factories",
            "Antares\\Jobx\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Antares\\Jobx\\Providers\\JobxServiceProvider"
            ]
        }
    }
}
